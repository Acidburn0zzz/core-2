{
    "name": "zikula/core1",
    "homepage": "http://zikula.org/",
    "license": "LGPL-3.0+",
    "description": "Zikula Application Framework",
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Zikula": ["src/lib", "src/lib/legacy"],
            "Zikula\\Module": "src/system",
            "Zikula\\Theme": "src/themes",
            "DoctrineExtensions\\StandardFields": "src/lib/legacy",
            "CustomBundle\\": "src/app/custom"
        },
        "classmap": [
            "src/lib/util/",
            "src/lib/i18n/",
            "src/lib/legacy/dbobject"
        ],
        "files": [
            "src/lib/i18n/ZGettextFunctions.php",
            "src/lib/vendor/idn/idna_convert.class.php",
            "src/lib/vendor/encryption/rc4crypt.class.php",
            "src/lib/legacy/Loader.php"
        ]
    },
    "authors": [
        {
            "name": "Drak",
            "email": "drak@zikula.org"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "doctrine/doctrine-bundle": "1.2.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "drak/doctrine1": "dev-master",
        "drak/smarty": "dev-master",
        "phpids/phpids": "dev-master",
        "gedmo/doctrine-extensions": "2.3.*",
        "imagine/Imagine": "dev-master",
        "michelf/php-markdown": "1.3.*@dev",
        "sensio/distribution-bundle": "2.2.*",
        "sensio/framework-extra-bundle": "2.2.*",
        "stof/doctrine-extensions-bundle": "1.1.*",
        "symfony/swiftmailer-bundle": "2.2.*",
        "symfony/assetic-bundle": "2.1.*",
        "symfony/browser-kit": "2.3.*",
        "symfony/class-loader": "2.3.*",
        "symfony/console": "2.3.*",
        "symfony/css-selector": "2.3.*",
        "symfony/doctrine-bridge": "2.3.*",
        "symfony/finder": "2.3.*",
        "symfony/filesystem": "2.3.*",
        "symfony/form": "2.3.*",
        "symfony/framework-bundle": "2.3.*",
        "symfony/http-foundation": "2.3.*",
        "symfony/monolog-bundle": "~2.2",
        "symfony/process": "2.3.*",
        "symfony/twig-bundle": "2.3.*",
        "symfony/web-profiler-bundle": "2.3.*",
        "symfony/validator": "2.3.*",
        "symfony/yaml": "2.3.*",
        "twig/twig": "<2.0",
        "zikula/filesystem": "dev-master"
    },
    "config": {
        "vendor-dir": "src/vendor",
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "src/app",
        "symfony-web-dir": "src/web"
    }
}
