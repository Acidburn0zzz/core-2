<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="zikulasearchmodule.event_listener.class">Zikula\Module\SearchModule\Listener\CoreEventListener</parameter>
    </parameters>

    <services>
        <service id="zikulasearchmodule.event_listener" class="%zikulasearchmodule.event_listener.class%">
            <tag name="kernel.event_subscriber" />
        </service>
    </services>
</container>

